#!/usr/bin/env php
<?php

require __DIR__ . "/vendor/autoload.php";

use OSN\Framework\Commands\MakeCommand;
use OSN\Framework\Commands\MigrateCommand;
use OSN\Framework\Console\App;

$app = new App(__DIR__);
$app->commander->register(MigrateCommand::class);
$app->commander->register(MakeCommand::class);
$app->run();
